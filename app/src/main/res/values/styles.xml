<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="text_style_1">
        <item name="android:layout_width">185dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#636363</item>
        <item name="android:gravity">right</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="et_edit_style_1">
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">42dp</item>
        <item name="android:layout_weight">2</item>
        <item name="android:paddingStart">8dp</item>
        <item name="android:paddingEnd">8dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">@drawable/selector_config_input_bg</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="config_linear_item_style">
        <item name="android:layout_width">600dp</item>
        <item name="android:layout_height">65dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_marginEnd">10dp</item>
    </style>


    <style name="status_rl_right_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_height">50dp</item>
    </style>

    <style name="status_tv_title">
        <item name="android:layout_width">220dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">18sp</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>



    <style name="chip_qos_selection_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:checkable">true</item>
        <item name="android:textColor">#323232</item>
        <item name="android:textSize">12sp</item>
        <item name="checkedIconTint">#2c63aa</item>
        <item name="chipBackgroundColor">@color/selector_chip_bg_qos</item>
        <item name="chipStrokeColor">#2c639a</item>
        <item name="chipStrokeWidth">0.8dp</item>
    </style>

    <style name="chip_group_qos_selection_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="chipSpacingHorizontal">10dp</item>
        <item name="singleSelection">true</item>
    </style>

    <style name="et_input_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:background">@drawable/selector_input_bg</item>
        <item name="android:gravity">left|top</item>
        <item name="android:paddingStart">8dp</item>
        <item name="android:paddingEnd">8dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
    </style>

    <style name="status_iv_state">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_marginEnd">35dp</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="line_view_horizontal_color_bbb">
        <item name="android:background">#BBBBBB</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.8dp</item>
    </style>
</resources>